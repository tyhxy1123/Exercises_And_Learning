<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="diAutowireByType" class="com.javacode2018.lesson001.demo1.DiAutowireByType" autowire="byType">
        <property name="service1">
            <bean class="com.javacode2018.lesson001.demo1.DiAutowireByType.Service1" autowire="byType">
                <property name="desc" value="service1 in diByType"></property>
            </bean>
        </property>
        <property name="service2">
            <bean class="com.javacode2018.lesson001.demo1.DiAutowireByType.Service2" autowire="byType">
                <property name="desc" value="service2 in diByType"></property>
            </bean>
        </property>
    </bean>
</beans>